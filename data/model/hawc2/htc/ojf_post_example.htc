    ; all hubs are fixed to the first node of the nose_cone
    begin fix1;
        mbdy1 nose_cone 1;
        mbdy2 hub1 1;
    end fix1;
;
    begin fix1;
        mbdy1 nose_cone 1;
        mbdy2 hub2 1;
    end fix1;
;
    begin fix1;
        mbdy1 nose_cone 1;
        mbdy2 hub3 1;
    end fix1;
