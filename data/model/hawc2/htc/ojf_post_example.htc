    ; free yaw bearing at the tower base
;;    begin bearing1;
;;        name  yaw_rot ;
;;        mbdy1 tower_base last;
;;        mbdy2 tower 1;
;;        ; coordinate system (0=global,1=mbdy1,2=mbdy2)
;;        ; and 3D bearing vector
;;        bearing_vector 1  0  0  1;
;;    end bearing1;
    ;
    ; ==========================================================================
    ; TOWER WITH SUPPORT STRUCTURE
    ;
    begin fix0;
        mbdy tower_support_p1;
    end fix0;
    ;
    begin fix1;
        mbdy1 tower_support_p1 last;
        mbdy2 tower_support_p12 1;
    end fix1;
    ;
    begin fix1;
        mbdy1 tower_support_p12 last;
        mbdy2 tower_support_p2 1;
    end fix1;
