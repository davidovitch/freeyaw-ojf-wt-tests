    ;
    ; SHAFT - NOSE_CONE
    ; fixed rotor speed
    begin bearing3;
         name shaft_nacelle;
         mbdy1 nacelle last;
         mbdy2 shaft 1;
         bearing_vector 2  0  0  -1;
         ; 300 RPM = 31.41 rad/s, 400 RPM = 41.89 rad/s,
         ; 600 RPM = 62.83 rad/s, 800 RPM = 83.78 rad/s
         omegas 7.41382773972 ;
