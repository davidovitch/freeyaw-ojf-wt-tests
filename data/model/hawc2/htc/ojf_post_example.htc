begin output;
    filename results/ojf_post_example;
    time 0.0 15.0;
    buffer 10000 ;
    general time ;
    data_format  gtsdf;
    ; ==========================================================================
    ; DASHBOARD OUTPUTS
    ; ==========================================================================
    ; ------------------------------------------------
    ; GLOBAL TURBINE PERFORMANCE
    ; ------------------------------------------------
    aero omega # Rotor speed [rad/s] ;
;    aero torque # Aerodynamic torque [kNm] ;
;    aero power # Aerodynamic power [kW] ;
;    ; Integrated aerodynamic forces of all blades to rotor thrust. Unit [kN].
;    aero thrust # Aerodynamic thrust [kN] ;
    aero azimuth 1;
    ; ------------------------------------------------
    ; WIND GLOBAL
    ; ------------------------------------------------
    wind free_wind     1 0  0 -2.305 only 2 # Free Wind Speed [m/s] ;
    wind free_wind_hor 1 0  0 -2.305 only 2 # Wind direction [deg] ;
    ; ------------------------------------------------
    ; Blade
    ; ------------------------------------------------
    aero vrel 1 0.04;
    aero vrel 1 0.111;
    aero vrel 1 0.167;
    aero vrel 1 0.278;
    aero vrel 1 0.51;
    ;
    aero alfa 1 0.04;
    aero alfa 1 0.111;
    aero alfa 1 0.167;
    aero alfa 1 0.278;
    aero alfa 1 0.51;
    ;
    aero cl   1 0.04;
    aero cl   1 0.111;
    aero cl   1 0.167;
    aero cl   1 0.278;
    aero cl   1 0.51;
    ; pitching, coning frames of reference
    ; refer to node number 0 or 1, not 1 and 2 because they are the same
    ; difference between local and blade1/2 frame of reference is small.
    ; Further, I don't know exactly what the difference is between the airfoil
    ; pitch angle and the beam pitch orientation. The difference is small anyway
    ; and the difference between blade1/2 and local (a very small offset)
    ; is also much smaller than the big difference we see with the measurements.
    mbdy momentvec blade1 1 1 blade1 # blade1 root strain gauge;
    mbdy momentvec blade1 4 1 blade1 # blade1 30% strain gauge ;
    mbdy momentvec blade2 1 1 blade2 # blade2 root strain gauge;
    mbdy momentvec blade2 4 1 blade2 # blade2 30% strain gauge ;
    ; forces
    mbdy forcevec  blade1 1 1 blade1 # blade1 root strain gauge;
    mbdy forcevec  blade1 4 1 blade1 # blade1 30% strain gauge ;
    mbdy forcevec  blade2 1 1 blade2 # blade2 root strain gauge;
    mbdy forcevec  blade2 4 1 blade2 # blade2 30% strain gauge ;
    ; blade tip displacements
    mbdy state pos blade1 11 1.0 hub1 # blade1 tip;
    mbdy state pos blade2 11 1.0 hub2 # blade2 tip;
    ; blade tip rotation
    aero rotation 1   3   0.555  1;
    aero rotation 2   3   0.555  1;
    ; ------------------------------------------------
    ; YAW angle
    ; ------------------------------------------------
;    constraint bearing1 yaw_rot 5 only 1 # Yaw angle [deg];
;    constraint bearing2 yaw_rot 5 only 1 # Yaw angle  [deg];
;;    constraint bearing2 yaw_rot 5 only 2 # Yaw rate  [deg/s];
    ; make sure all channels in the output remain in the same place
    ; add zero channels as replacement for yaw angle and speed
;    general constant 0 ;
;    general constant 0 ;
    ; ------------------------------------------------
    ; TOWER
    ; ------------------------------------------------
    ; rotating (free yaw) and non rotating frames of reference
    mbdy momentvec tower  1 1 tower  # node_nr_2 tower base strain gauges ;
    mbdy momentvec tower  1 1 global # node_nr_2 tower base strain gauges ;
    ; forces
    mbdy forcevec  tower  1 1 tower  # node_nr_2 tower base strain gauges ;
    mbdy forcevec  tower  1 1 global # node_nr_2 tower base strain gauges ;
    ; tower top accelerations
    mbdy state acc tower  3 1.0 tower # tower top accelerations;
