begin simulation;
    time_stop    15.0;
    solvertype   1 ;    (newmark)
    on_no_convergence continue ;
    convergence_limits  1e-5  1e-6  7e-7 ;
;    1. epsresq, residual on internal-external forces,
;    default=10.0
;    2. epsresd, residual on increment, default=1.0
;    3. epsresg, residual on constraint equations,
;    default=0.7
    logfile ./logfiles/ojf_post_example.log ;
;    animation ./animation/ojf_post_example.dat;
;
    begin newmark;
        deltat    0.00075;
    end newmark;
end simulation;
